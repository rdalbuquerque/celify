{
    "spec": {
        "template": {
            "metadata": {
                "labels": {
                    "app": "my-app"
                }
            },
            "spec": {
                "containers": [
                    {
                        "name": "my-container",
                        "resources": {
                            "limits": {
                                "memory": "128Mi",
                                "cpu": "500m"
                            },
                            "requests": {
                                "memory": "128Mi",
                                "cpu": "250m"
                            }
                        },
                        "image": "my-image:1.2.3"
                    }
                ]
            }
        },
        "replicas": 55
    },
    "metadata": {
        "name": "test-deployment"
    },
    "apiVersion": "apps/v1",
    "kind": "Deployment"
}